ScreenManager:
    Screen:
        name: "init"
        MDBoxLayout:
            orientation: "vertical"
            md_bg_color:.77,.89,.88,.83
            MDLabel:
                text: f"[color=#000000BB]F1[/color]-Guess\nThe Pole Time"
                color:app.theme_cls.primary_color
                halign: "center"
                markup: True
                font_style: "H3"
            RelativeLayout:
                size_hint_y: .4
                MDProgressBar:
                    id: spinner
                    type: "determinate"
                    size_hint_x:.5
                    size_hint_y: None
                    height: dp(2)
                    pos_hint:{"center_x": .5, "center_y": .5}
                    on_kv_post:
                        self.start()
    Screen:
        id: manager
        name: "manager"