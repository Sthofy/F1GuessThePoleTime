<MainWidget>:
    BoxLayout:
        size_hint_x: .3
        orientation:'vertical'
        canvas:
            Color:
                rgb:0.11,0.24,0.30
            Rectangle:
                size:self.size
                pos:self.pos
        Button:
            text:'Log out'
            size: 100,50
            size_hint: None, None
            on_release:
                app.root.current = 'login'
                root.manager.transition.direction = 'right'
    BoxLayout:
        size_hint_x: .7
